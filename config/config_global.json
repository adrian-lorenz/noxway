{
  "ssl": false,
  "debug": true,
  "excludedPaths": [],
  "port": "8080",
  "sslPort": "443",
  "cors": true,
  "rateLimiter": true,
  "bann": true,
  "metric": true,
  "prefix": "/v1/",
  "pemCrt": "/etc/letsencrypt/live/deine-domain.de/fullchain.pem",
  "pemKey": "/etc/letsencrypt/live/deine-domain.de/privkey.pem",
  "metricPath": "/metrics",
  "metricWhitelist": [
    "127.0.0.1"
  ],
  "bannlist": [],
  "rate": {
    "rate": 500,
    "window": 3600000000000
  },
  "rateWhitelist": [
    "127.0.0.1"
  ],
  "corsAllowOrigins": [
    "*"
  ],
  "corsAllowMethods": [
    "GET",
    "POST",
    "PUT",
    "DELETE",
    "OPTIONS"
  ],
  "corsAllowHeaders": [
    "*"
  ],
  "corsAdvanced": true,
  "exportLog": false,
  "exportLogPath": "/var/log/noxway.log",
  "hostnamecheck": true,
  "hostname": "127.0.0.1",
  "name": "Gateway 2"
}